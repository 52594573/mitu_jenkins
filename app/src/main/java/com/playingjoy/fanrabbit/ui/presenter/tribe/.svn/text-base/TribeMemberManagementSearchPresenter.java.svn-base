package com.playingjoy.fanrabbit.ui.presenter.tribe;

import com.playingjoy.fanrabbit.base.BasePresenter;
import com.playingjoy.fanrabbit.ui.activity.tribe.TribeMemberManagementSearchActivity;

import java.util.ArrayList;
import java.util.List;

/**
 * 部落成员管理搜索控制器
 *
 * @author Morphine
 * @date 2018-04-18.
 */

public class TribeMemberManagementSearchPresenter extends BasePresenter<TribeMemberManagementSearchActivity> {

    /**
     * 搜索成员
     *
     * @param keyword
     */
    public void searchUser(String keyword) {
        List<String> list = new ArrayList<>();
        for (int i = 0; i < 10; i++) {
            list.add("这是搜索的成员" + i);
        }
        getV().setSearchList(list);
    }
}
